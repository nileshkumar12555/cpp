// C++ program to check if a given 
// year is leap year or not 

#include<iostream>
using namespace std;

int CheckLeap(int year)
{
    //check leap year
    if(year%400==0)
    return true;

    //not leap year
    if(year%100==0)
    return false;

    // leap year 
    if (year % 4 == 0) 
        return true; 
  
    // Not leap year 
    return false;

}
int main()
{
    int year=2000;

    if(CheckLeap(year))

    cout<<"leap year";
    else
    cout<<"not leap year";
}
