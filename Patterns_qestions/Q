#---------------#
#include<iostream>
using namespace std;

int main()
{
    int n;
    cin>>n;
    for(int row=0;row<=n;row++)
    {
        for(int column=0;column<=n;column++)
        {
            cout<<"*"<<" ";
        }
        cout<<endl;
    }    
}
