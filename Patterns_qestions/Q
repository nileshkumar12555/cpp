10)
1
2 3
3 4 5
4 5 6 7

#include<iostream>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int toprint=1;
    for(int row=1;row<=n;row++)
    {
        toprint=row;
        for(int column=1;column<=row;column++)
        {
            
            cout<<toprint<<" ";
            toprint=toprint+1;
           
        }
        cout<<endl;
    }    
}

11)
1
2 1
3 2 1
4 3 2 1

#include<iostream>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int toprint=1;
    for(int row=1;row<=n;row++)
    {
        for(int column=1;column<=row;column++)
        {
            
            cout<<(row-column+1)<<" ";
           
        }
        cout<<endl;
    }    
}

12)
A A A A
B B B B
C C C C
D D D D

#include<iostream>
using namespace std;

int main()
{
    int n;
    cin>>n;
    for(int row=1;row<=n;row++)
    {
        for(int column=1;column<=n;column++)
        {
            
            cout<<(char)(65+row-1)<<" ";
           
        }
        cout<<endl;
    }    
}

13)
A B C D
A B C D
A B C D
A B C D

#include<iostream>
using namespace std;

int main()
{
    int n;
    cin>>n;
    for(int row=1;row<=n;row++)
    {
        char ch='A';
        for(int column=1;column<=n;column++)
        {
            
            cout<<ch<<" ";
            ch=ch+1;
        }
        cout<<endl;
    }    
}


















