#include<iostream>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int toprint=1;
    for(int row=1;row<=n;row++)
    {
        for(int column=1;column<=n;column++)
        {
            cout<<toprint<<" ";
            toprint=toprint+1;
        }
        cout<<endl;
    }    
}
